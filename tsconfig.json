{
  "extends": "astro/tsconfigs/base",
  "compilerOptions": {
    "strictNullChecks": true,
    "allowJs": true,
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "types": ["node"],
    "moduleResolution": "node",
    "plugins": [
        {
            "name": "@astrojs/ts-plugin",
        },
    ],
  },
  "include": [
    "src/**/*.ts"
  ],
  "exclude": [
      "node_modules"
  ],
}
